package org.parafia.util;

public class StringUtil {
	public static String getSpaces(String text, float multiply) {
		if (text != null) {
			StringBuffer sb = new StringBuffer();
			for (int i = 0; i < (int)(text.length() * multiply); i++)
				sb.append(' ');
			return sb.toString();
		} else
			return null;
	}
}
